material {
  name : ground,
  requires : [ uv0 ],
  shadingModel : unlit,
  parameters : [
    { type : sampler2d, name : baseColor }
  ],
}

fragment {
  void material(inout MaterialInputs material) {
    prepareMaterial(material);
    material.baseColor = texture(materialParams_baseColor, getUV0());
  }
}
